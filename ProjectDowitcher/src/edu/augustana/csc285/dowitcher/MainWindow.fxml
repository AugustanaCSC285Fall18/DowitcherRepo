      </AnchorPane>
      <Slider fx:id="sliderSeekBar" layoutX="174.0" layoutY="585.0" prefHeight="14.0" prefWidth="454.0" />
      <TextArea fx:id="currentFrameArea" layoutX="340.0" layoutY="608.0" prefHeight="38.0" prefWidth="305.0" />
      <TextArea fx:id="totalFrameArea" layoutX="160.0" layoutY="608.0" prefHeight="5.0" prefWidth="154.0" wrapText="true" />
      <Label fx:id="jumpToFrame" layoutX="266.0" layoutY="684.0" prefHeight="26.0" prefWidth="95.0" text="Jump to frame:" />
