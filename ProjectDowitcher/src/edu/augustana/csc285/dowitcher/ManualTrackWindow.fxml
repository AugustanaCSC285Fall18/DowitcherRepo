<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="appArea" prefHeight="724.0" prefWidth="827.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.csc285.dowitcher.ManualTrackWindowController">
   <children>
      <AnchorPane fx:id="currentFrameWrapper" layoutX="10.0" layoutY="46.0">
         <children>
            <ImageView fx:id="currentFrameImage" fitHeight="440.0" fitWidth="660.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
      <Slider fx:id="sliderSeekBar" layoutX="5.0" layoutY="503.0" prefHeight="14.0" prefWidth="660.0" />
      <TextArea fx:id="currentFrameArea" layoutX="253.0" layoutY="531.0" prefHeight="38.0" prefWidth="305.0" />
      <TextArea fx:id="totalFrameArea" layoutX="73.0" layoutY="531.0" prefHeight="5.0" prefWidth="154.0" wrapText="true" />
      <Label fx:id="jumpToFrame" layoutX="257.0" layoutY="583.0" prefHeight="26.0" prefWidth="95.0" text="Jump to frame:" />
      <TextField fx:id="jumpToFrameArea" layoutX="340.0" layoutY="583.0" prefHeight="26.0" prefWidth="218.0" />
      <Button fx:id="redoButton" layoutX="318.0" layoutY="14.0" mnemonicParsing="false" text="Redo" />
      <MenuButton fx:id="chooseChickMenu" layoutX="680.0" layoutY="169.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="114.0" text="Choose Chick To Track:" />
      <Button fx:id="finishManualTrackingBtn" layoutX="380.0" layoutY="637.0" mnemonicParsing="false" onAction="#handleFinishManualTracking" text="Finish Manual Tracking" />
      <TextField fx:id="setNameTextField" layoutX="681.0" layoutY="135.0" prefHeight="26.0" prefWidth="114.0" />
      <Button fx:id="setNameButton" layoutX="756.0" layoutY="102.0" mnemonicParsing="false" onAction="#handleSetNameButton" prefHeight="26.0" prefWidth="38.0" text="Set" />

   </children>
</AnchorPane>
