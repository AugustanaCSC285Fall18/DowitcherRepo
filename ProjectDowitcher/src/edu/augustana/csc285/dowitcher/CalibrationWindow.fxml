<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="appArea" prefHeight="724.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.csc285.dowitcher.CalibrationWindowController">
   <children>
      <ImageView fx:id="currentFrameImage" fitHeight="440.0" fitWidth="660.0" layoutX="21.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true" />
      <Slider fx:id="sliderSeekBar" layoutX="22.0" layoutY="506.0" prefHeight="14.0" prefWidth="658.0" />
      <TextArea fx:id="currentFrameArea" layoutX="316.0" layoutY="14.0" prefHeight="38.0" prefWidth="305.0" />
      <TextArea fx:id="totalFrameArea" layoutX="123.0" layoutY="14.0" prefHeight="5.0" prefWidth="154.0" wrapText="true" />
      <TextField fx:id="startFrame" layoutX="99.0" layoutY="585.0" />
      <TextField fx:id="endFrame" layoutX="320.0" layoutY="585.0" />
      <TextField fx:id="numChicks" layoutX="554.0" layoutY="585.0" />
      <Label layoutX="44.0" layoutY="589.0" text="startFrame" />
      <Label layoutX="493.0" layoutY="589.0" text="# of Chicks" />
      <Label layoutX="266.0" layoutY="589.0" text="endFrame" />
      <Button fx:id="submitBtn" layoutX="293.0" layoutY="654.0" mnemonicParsing="false" onAction="#handleSubmit" prefHeight="38.0" prefWidth="82.0" text="Submit" />

   </children>
</AnchorPane>
